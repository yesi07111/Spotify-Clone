{
  "name": "spotify-clone-ui",
  "version": "1.0.0",
  "description": "A Spotify-like music player application with playback, search, and playlist management features",
  "author": "Yesenia Valdés Rodríguez, Olivia Ibañez Mustelier",
  "license": "MIT",
  "private": true,
  "main": "src/main.js",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.3",
    "core-js": "^3.8.3",
    "crypto-js": "^4.2.0",
    "howler": "^2.2.4",
    "http-proxy": "^1.18.1",
    "mitt": "^3.0.1",
    "vue": "^3.5.25",
    "vue-router": "^4.4.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "@vue/compiler-sfc": "^3.5.25",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true,
      "es2021": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser",
      "requireConfigFile": false,
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
